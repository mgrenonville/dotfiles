% Insert the clipboard buffer in current window
"xsel -x; xsel -o -b | xsel -i; xdotool click 2; xsel -x"
    Alt + Insert
% Copy Primary to Clipboard
" xsel -p | xsel -i -b"
    Alt + End
% Copy Clipboard to Primary
" xsel -b | xsel -i -p"
    Alt + Delete

"xbacklight -dec 5"
  XF86MonBrightnessDown
"xbacklight -inc 5"
  XF86MonBrightnessUp
# Increase volume
"pactl set-sink-volume @DEFAULT_SINK@ +1000"
   XF86AudioRaiseVolume
# Decrease volume
"pactl set-sink-volume @DEFAULT_SINK@ -1000"
   XF86AudioLowerVolume
# Mute volume
"pactl set-sink-mute @DEFAULT_SINK@ toggle"
   XF86AudioMute
# Mute microphone
"pactl set-source-mute @DEFAULT_SOURCE@ toggle"
   XF86AudioMicMute
